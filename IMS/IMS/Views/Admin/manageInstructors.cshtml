@model  IEnumerable<AspNetUser>
@{
    ViewBag.Title = "Manage Instructors";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
<div>
    <table style="width:100%">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Contact</th>
            <th>CNIC</th>
            <th>City</th>
            <th>Date Of Birth</th>
            <th>RegistrationDate</th>
            <th>Assign Courses</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FirstName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastName)
                </td>
               
                    

                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Contact)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Cnic)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.City)
                </td>
                <td>
                    @(item.DOB.Day.ToString() + '/' + item.DOB.Month.ToString() + '/' + item.DOB.Year.ToString())
                </td>
                <td>
                    @(item.RegistrationDate.Day.ToString() + '/' + item.RegistrationDate.Month.ToString() + '/' + item.RegistrationDate.Year.ToString())
                </td>
                <td>
                    @Html.ActionLink("AssignCourse", "AssignCourse", new { Id = item.Id })
                </td>
            </tr>
        }
    </table>
</div>


